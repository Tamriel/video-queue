{
  "name": "video-queue",
  "version": "1.0.0",
  "description": "A desktop video player that remembers your progress, lets you control playback order and organize videos files into themed folders.",
  "main": "./out/main/main.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Tamriel/video-queue"
  },
  "author": {
    "name": "Tamriel",
    "url": "https://github.com/Tamriel"
  },
  "scripts": {
    "dev": "electron-vite dev -w",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "start": "electron-vite preview",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "electron-vite build && electron-builder --dir",
    "build:win": "electron-vite build && electron-builder --win",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux",
    "test": "jest"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "electron-conf": "^1.3.0",
    "video.js": "^8.22.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.0.0",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@rushstack/eslint-patch": "^1.10.5",
    "@tailwindcss/vite": "^4.0.9",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^34.3.0",
    "electron-builder": "^25.1.8",
    "electron-vite": "^3.0.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react": "^7.37.4",
    "framer-motion": "^12.4.7",
    "jest": "^29.7.0",
    "prettier": "^3.5.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.9",
    "ts-jest": "^29.3.4",
    "typescript": "^5.8.3",
    "vite": "^6.2.0"
  }
}
